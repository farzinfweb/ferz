{
  "name": "@ferz/persian-calendar",
  "version": "0.0.2",
  "description": "The persian calendar for ferz library",
  "keywords": [
    "date",
    "immutable",
    "calendar",
    "persian",
    "jalali"
  ],
  "homepage": "https://github.com/farzinfweb/ferz/tree/main/packages/calendars-persian#readme",
  "bugs": {
    "url": "https://github.com/farzinfweb/ferz/issues"
  },
  "license": "MIT",
  "author": "Farzin Far <farzin.farzanehnia@gmail.com>",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/farzinfweb/ferz.git",
    "directory": "packages/ferz"
  },
  "funding": "https://github.com/sponsors/farzinfweb",
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/index.cjs",
      "import": "./dist/index.mjs",
      "types": "./dist/index.d.ts"
    },
    "./locales/*": {
      "types": "./locales/*.d.ts",
      "import": "./locales/*.mjs"
    }
  },
  "files": [
    "dist",
    "locales",
    "*.d.ts",
    "README.md"
  ],
  "sideEffects": false,
  "scripts": {
    "build": "rimraf -g locales && unbuild",
    "stub": "unbuild --stub"
  },
  "devDependencies": {
    "@ferz/shared": "workspace:*"
  },
  "engines": {
    "node": ">=16"
  }
}
